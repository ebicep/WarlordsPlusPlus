{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_authors}"
  ],
  "contact": {
    "homepage": "https://github.com/ebicep/WarlordsPlusPlus/",
    "sources": "https://github.com/ebicep/WarlordsPlusPlus/",
    "issues": "https://github.com/ebicep/WarlordsPlusPlus/issues/"
  },
  "license": "GPL-3.0",
  "icon": "assets/warlordsplusplus/icon.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.ebicep.warlordsplusplus.fabric.WarlordsPlusPlusFabric"
      }
    ]
  },
  "mixins": [
    "warlordsplusplus.mixins.json",
    "warlordsplusplus-common.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "minecraft": ">=${minecraft_version}",
    "architectury": ">=${architectury_version}",
    "fabric-language-kotlin": ">=${fabric_kotlin_version}"
  }
}